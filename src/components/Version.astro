---
import { getDefaultVersion, resolveVersion } from '../composables/version'
import type { Type, Version } from '../types/version.ts'

interface Props {
    type: Type
    version: Version
}

const { type = 'current', version = getDefaultVersion(Astro.url) } = Astro.props
const fullVersion = resolveVersion(version, type)
---

{fullVersion}
