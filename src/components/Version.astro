---
import { getDefaultVersion, resolveFullVersion } from '../composables/version'
import type { Type, Version } from '../types/version'

interface Props {
    type?: Type
    version?: Version
}

const { type = 'current', version = getDefaultVersion(Astro.url) } = Astro.props
const fullVersion = resolveFullVersion(version, type)
---

{fullVersion}
