{
  "name": "website",
  "scripts": {
    "astro:build": "astro build",
    "astro:check": "astro check",
    "astro:start": "astro dev",
    "cspell:check": "cspell \"*.md\" \"src/**/*.mdx\" --no-progress --no-summary --config \".cspell.yaml\"",
    "eslint:check": "eslint \"*.mjs\" \"src/**/*.astro\" \"src/**/*.ts\"",
    "eslint:fix": "eslint \"*.mjs\" \"src/**/*.astro\" \"src/**/*.ts\" --fix",
    "remark:check": "remark . --ext .md,.mdx --frail --quiet",
    "remark:format": "remark . --ext .md,.mdx --frail --quiet --output",
